          ]
        },
        {
          "id": "MAracutaia",
          "name": "Abrir maracutaia",
          "prefix": "plugin",
          "type": "button",
          "tryInline": true,
          "isForm": false,
          "format": "Abrir maracutaia {$CONFIG_DIR$}",
